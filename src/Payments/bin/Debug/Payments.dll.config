<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
        <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
            <section name="Payments.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
        </sectionGroup>
    </configSections>
    <connectionStrings>
        <add name="Payments.Properties.Settings.PayPalDb" connectionString="you_must_define_your_own_db" />
    </connectionStrings>
    <applicationSettings>
        <Payments.Properties.Settings>
            <setting name="PayPalApiUsername" serializeAs="String">
                <value>your api username. E.g.: esesel_1263685459_biz_api1.computer.org</value>
            </setting>
            <setting name="PayPalApiPassword" serializeAs="String">
                <value>your api password. E.g.: FXNAHJBTYDV78EKY</value>
            </setting>
            <setting name="PayPalApiSignature" serializeAs="String">
                <value>your api signature. E.g.: Av1EF0H5TSuNjzjJE9jxc9HZacPmA40e7pFXXvJvGQatESCiRfVIyQoW</value>
            </setting>
            <setting name="PayPalApiEnvironment" serializeAs="String">
                <value>your api environment. E.g.: sandbox</value>
            </setting>
            <setting name="PayPalApiServer" serializeAs="String">
                <value>your api server. E.g.: http://www.sandbox.paypal.com</value>
            </setting>
            <setting name="PayPalApiReturn" serializeAs="String">
                <value>your api return. E.g.: http://localhost/CheckoutReview.aspx?operation=confirm</value>
            </setting>
            <setting name="PayPalApiCancel" serializeAs="String">
                <value>your api cancel. E.g.: http://localhost/CheckoutCancel.aspx?operation=cancel</value>
            </setting>
        </Payments.Properties.Settings>
    </applicationSettings>
</configuration>